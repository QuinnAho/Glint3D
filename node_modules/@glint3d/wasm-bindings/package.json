{
  "name": "@glint3d/wasm-bindings",
  "version": "0.3.0",
  "description": "WebAssembly bindings for Glint3D engine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
