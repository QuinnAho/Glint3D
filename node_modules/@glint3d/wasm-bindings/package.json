    "webassembly",
    "wasm",
    "emscripten",
    "3d",
    "graphics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/glint3d",
    "directory": "packages/wasm-bindings"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/emscripten": "^1.39.0"
  },
  "engines": {
    "node": ">=16"
  }
