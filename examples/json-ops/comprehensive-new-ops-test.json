{
  "version": 1,
  "ops": [
    {
      "op": "load",
      "path": "engine/assets/models/cube.obj",
      "name": "MainCube",
      "position": [0, 0, 0]
    },
    {
      "op": "duplicate",
      "source": "MainCube",
      "name": "DuplicatedCube",
      "position": [3, 0, 0],
      "rotation": [0, 45, 0]
    },
    {
      "op": "select",
      "name": "DuplicatedCube"
    },
    {
      "op": "set_camera_preset",
      "preset": "iso_fl",
      "target": [1.5, 0, 0],
      "fov": 50.0,
      "margin": 0.4
    },
    {
      "op": "orbit_camera",
      "yaw": 30.0,
      "pitch": 10.0,
      "center": [1.5, 0, 0]
    },
    {
      "op": "frame_object",
      "name": "MainCube",
      "margin": 0.3
    },
    {
      "op": "add_light",
      "type": "directional",
      "direction": [-1, -1, -1],
      "color": [1, 0.9, 0.8],
      "intensity": 1.5
    },
    {
      "op": "set_background",
      "color": [0.1, 0.15, 0.2]
    },
    {
      "op": "exposure",
      "value": -0.5
    },
    {
      "op": "tone_map",
      "type": "aces",
      "gamma": 2.2
    },
    {
      "op": "render_image",
      "path": "comprehensive_test.png",
      "width": 800,
      "height": 600
    }
  ]
}